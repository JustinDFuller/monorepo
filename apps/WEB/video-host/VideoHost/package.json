{
  "name": "VideoHost",
  "version": "1.0.0",
  "description": "VideoHost Backend",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/JustinDFuller/VideoHost.git",
    "type": "git"
  },
  "author": "Justin Fuller <justindanielfuller@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "ava": "^2.3.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inferno": "^6.1.0",
    "babel-preset-latest": "^6.16.0",
    "bcrypt": "^3.0.6",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "commitizen": "^4.0.3",
    "compression": "^1.6.2",
    "conventional-changelog-cli": "^2.0.23",
    "coveralls": "^3.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.1.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "forever": "^1.0.0",
    "ghooks": "^2.0.4",
    "helmet": "^3.1.0",
    "inferno": "7.3.1",
    "lodash": "^4.17.2",
    "mock-require": "^3.0.3",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "sinon": "^7.4.2",
    "webpack": "4.39.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "yarn run changelog && git add CHANGELOG.md"
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.*",
      "node_modules"
    ]
  },
  "scripts": {
    "start": "forever server",
    "dev": "nodemon server",
    "commit": "git-cz",
    "test": "nyc ava **/*.spec.js && yarn run coverage",
    "test:watch": "nyc ava **/*.spec.js --watch",
    "coverage": "nyc report --reporter=html",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build": "webpack"
  }
}
