{
    "name": "immutable-functional-react",
    "version": "1.0.0",
    "description": "React.. but immutable and functional. No classes.",
    "main": "index.js",
    "scripts": {
        "test": "ava **/*.spec.js",
        "test:watch": "npm test -- --watch",
        "examples": "babel examples --out-dir examples/build --source-maps",
        "lint": "eslint src tests --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JustinDFuller/immutable-functional-react.git"
    },
    "keywords": [
        "react",
        "javascript",
        "functional",
        "immutable",
        "browser",
        "dom"
    ],
    "author": "Justin Fuller <justindanielfuller@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/JustinDFuller/immutable-functional-react/issues"
    },
    "homepage": "https://github.com/JustinDFuller/immutable-functional-react#readme",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "immutable": "^3.8.2",
        "ramda": "^0.25.0",
        "virtual-dom": "^2.1.1"
    },
    "devDependencies": {
        "ava": "^0.24.0",
        "babel-cli": "^6.26.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-ava": "^4.4.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0"
    },
    "ava": {
        "babel": {
            "babelrc": true
        },
        "require": [
            "babel-polyfill",
            "babel-register"
        ]
    }
}
